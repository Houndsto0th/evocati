<!DOCTYPE html>
<html>
<head>
  <title>Evocati</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= csrf_meta_tags %>
  <%= javascript_include_tag "application.js", media: 'all' %>
  <link href='http://fonts.googleapis.com/css?family=Playfair+Display+SC:400,900,700,400italic' rel='stylesheet' type='text/css'>
</head>
<body id="page-top" class="index">

  <!-- Navigation -->
  <nav class="navbar navbar-default navbar-fixed-top">
    <div class="container">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header page-scroll">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand page-scroll" href="#page-top"><%= image_tag 'Evocati_logo.png' %></a>
      </div>

 <%= yield %>

      <!-- jQuery -->
      <!-- Bootstrap Core JavaScript -->
      <%= javascript_include_tag "bootstrap.min.js" %>
      <!-- Plugin JavaScript -->
      <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
      <%= javascript_include_tag "classie.js" %>
      <%= javascript_include_tag "cbpAnimatedHeader.js" %>
      <!-- Contact Form JavaScript -->
      <!-- Custom Theme JavaScript -->
      <%= javascript_include_tag "agency.js" %>

    </body>
    </html>

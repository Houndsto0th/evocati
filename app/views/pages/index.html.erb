<!-- Team Section -->
<section id="team" class= "bg-light-gray">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2 class="section-heading">Raid Team</h2>
        <h3 class="section-subheading text-muted">Armory links on the left | WarcraftLogs on the right.</h3>
      </div>
    </div>
    <div class="col-lg-12 text-center">
      <h3 class="section-heading">Tanks</h3>
    </div>
      <!--each loop here -->
      <% @tanks.each do |tank| %>
      <div class="col-sm-6">
        <div class="team-member">
          <h4><%= tank[:name] %></h4>
          <p class="text-muted"><%= tank[:spec] %></p>
          <ul class="list-inline social-buttons">
            <li><a href='http://us.battle.net/wow/en/character/<%= tank[:realm] %>/<%= tank[:name] %>/advanced'><i class="fa fa-linkedin"></i></a>
            </li>
            <li><%= image_tag "#{tank[:class]}.ico", class: "img-roster" %></a>
            </li>
            <li><a href='https://www.warcraftlogs.com/rankings/character/<%= tank[:warlogs] %>/latest'><i class="fa fa-linkedin"></i></a>
            </li>
          </ul>
        </div>
      </div>
      <% end %>
      <br>
      <br>
      <br>
      <div class="col-lg-12 text-center">
        <h3 class="section-heading">Healers</h3>
      </div>
      <% @healers.each do |healer| %>
      <div class="col-sm-4">
        <div class="team-member">
          <h4><%= healer[:name] %></h4>
          <p class="text-muted"><%= healer[:spec] %></p>
          <ul class="list-inline social-buttons">
            <li><a href='http://us.battle.net/wow/en/character/<%= healer[:realm] %>/<%= healer[:name] %>/advanced'><i class="fa fa-linkedin"></i></a>
            </li>
            <li><%= image_tag "#{healer[:class]}.ico", class: "img-roster" %></a>
            </li>
            <li><a href='https://www.warcraftlogs.com/rankings/character/<%= healer[:warlogs] %>/latest'><i class="fa fa-linkedin"></i></a>
            </li>
          </ul>
        </div>
      </div>
      <% end %>
      <br>
      <br>
      <br>
      <div class="col-lg-12 text-center">
        <h3 class="section-heading">DPS</h3>
      </div>
      <% @dps.each do |dps| %>
      <div class="col-sm-4">
        <div class="team-member">
          <h4><%= dps[:name] %></h4>
          <p class="text-muted"><%= dps[:spec] %></p>
          <ul class="list-inline social-buttons">
            <li><a href='http://us.battle.net/wow/en/character/<%= dps[:realm] %>/<%= dps[:name] %>/advanced'><i class="fa fa-linkedin"></i></a>
            </li>
            <li><%= image_tag "#{dps[:class]}.ico", class: "img-roster" %></a>
            </li>
            <li><a href='https://www.warcraftlogs.com/rankings/character/<%= dps[:warlogs] %>/latest'><i class="fa fa-linkedin"></i></a>
            </li>
          </ul>
        </div>
      </div>
      <% end %>
    </div>
  </section>



  <footer>
    <div class="container">
      <div class="row">
        <div class="col-md-4">
          <span class="copyright">Site built and maintained by Houndstooth.</span>
